{
    "api": {
        "text": "Enter API Key",
        "type": "textbox",
        "value": "OpenWeatherMap API Key"
    },
    "milTime": {
        "type": "checkbox",
        "value": true,
        "text": "24 Hour Time"
    },
    "numImg": {
        "text": "Number of images",
        "type": "slider",
        "max": 100,
        "min": 0,
        "step": 1,
        "value": 1 
    },
    "wallpaperType": {
        "text": "0 = random local, 1 = random online",
        "type": "slider",
        "max": 1,
        "min": 0,
        "step": 1,
        "value": 1 
    },
    "celsius": {
        "type": "checkbox",
        "value": false,
        "text": "Celsius"
    },
    "showSec": {
        "type": "checkbox",
        "value": true,
        "text": "Show Seconds"
    },
    "showAMPM": {
        "type": "checkbox",
        "value": true,
        "text": "Show AM/PM"
    },
    "showLyrics": {
        "type": "checkbox",
        "value": true,
        "text": "Show Lyrics"
    },
    "syncOffset": {
        "text": "Lyrics Sync Offset",
        "type": "slider",
        "max": 10,
        "min": -10,
        "step": 0.5,
        "value": 1 
    },
    "backgroundBlur": {
        "text": "Background Blur",
        "type": "slider",
        "max": 50,
        "min": 0,
        "step": 1,
        "value": 5 
    }
}